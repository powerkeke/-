#include<bits/stdc++.h>
using namespace std;
const int maxn=100;
int T[maxn],n,x;
void solve(int x){
	for(int i=1;i<=n;i++){
		if(T[i]==x){
			cout<<i<<"\n";
			return;//找到x，输出下标i 
		}
	}
	cout<<0<<"\n";//未找到，输出0 
}
int main(){
	cin>>n;//数组长度为n 
	for(int i=1;i<=n;i++)cin>>T[i];
	cin>>x;//查找x的下标 
	solve(x);
	return 0;
}
